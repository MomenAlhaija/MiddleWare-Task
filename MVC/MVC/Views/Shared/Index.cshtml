@model IEnumerable<Person>;
<!Doctype html>
<html>
    <head>
        <meta charset="utf-8" />
    <link href="~/StyleSheet.css" rel="stylesheet" />
    </head>
    <body>

     <h4>
       
        @foreach(var person in Model)
        {
            
            <h1 class="textCenter">Name:@person.Name</h1>
            <h3 class="textCenter">DateOFBirth:@person.DateOfBirth.ToString("yyyy-MM-dd")</h3>
            <a href="/Person-detailes/@person.Name">Detailes</a>
            <a href="/Person-With-Product">Person And Product</a>
        }
   

        
    </h4>
    </body>
</html>
